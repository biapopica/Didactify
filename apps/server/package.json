{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "type": "module",
  "license": "ISC",
  "description": "",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --platform=node --packages=external --outfile=dist/server.js --format=esm",
    "start": "node dist/server.js",
    "dev": "concurrently \"npm run dev:ts\" \"npm run dev:build\" \"npm run dev:node\"",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "dev:build": "esbuild --watch src/server.ts --bundle --platform=node --packages=external --outfile=dist/server.js --format=esm",
    "dev:node": "node --watch dist/server.js"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "axios": "^1.13.2",
    "better-auth": "^1.3.34",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "concurrently": "^9.2.1",
    "esbuild": "^0.26.0",
    "prisma": "^7.0.1",
    "typescript": "^5.9.3"
  }
}
